# update
dic= {'mouse':'chuot','computer':'may tinh','keyboard':'ban phim'}


while True:
    word=str(input('search word or choose exit or quit '))
    dich= dic.get(word)
    if word in dic:
        print(dich)
    elif word== 'exit'or 'quit':
        print('thank you')
        break
    
        
    else: 
        print("don't have this word")
        

# duyet theo key
for i in dic:
    print(i)
# duyet theo value
for i in dic.value():
    print(i)
# duyet theo key-value
for i,v in dic.item:
    print(i,':',v)   

